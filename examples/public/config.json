{
  "$schema": "flux-eco-ui-map-element/schemas/component.json",
  "name": "example-ui-map",
  "configs": {
    "settings": {
      "mapElementCssLinkState": {
        "href": "flux-eco-ui-map-element/definitions/flux-eco-ui-map-element.css",
        "rel": "stylesheet"
      },
      "mapContainerDimensions": {
        "width": "800px",
        "height": "800px"
      }
    },
    "initialState": {
      "mapElementViewState": {
        "center": {
          "lat": 34.238972,
          "lng": -118.433449
        },
        "zoom": 13
      },
      "mapElementLayerStates": [
        {
          "layerOptions": {
            "attribution": "&copy; <a href=\"https://www.openstreetmap.org/copyright\">OpenStreetMap</a> contributors"
          },
          "layerUrl": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png"
        }
      ],
      "mapElementMarkerStates": []
    }
  }
}